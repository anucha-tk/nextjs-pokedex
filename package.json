{
	"name": "ts-nextjs-tailwind-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"typecheck": "tsc --noEmit --incremental false",
		"test:watch": "jest --watch",
		"test": "jest",
		"postbuild": "next-sitemap --config next-sitemap.config.js",
		"lint": "biome check src --no-errors-on-unmatched",
		"lint:strict": "biome check src --no-errors-on-unmatched --files-ignore-unknown=true",
		"check": "biome check src --write",
		"cz": "cz"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.5",
		"@heroicons/react": "^2.0.13",
		"@reduxjs/toolkit": "^1.8.5",
		"clsx": "^1.2.1",
		"install": "^0.13.0",
		"next": "^12.2.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.4.0",
		"react-redux": "^8.0.2",
		"tailwind-merge": "^1.6.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@commitlint/cli": "^16.3.0",
		"@commitlint/config-conventional": "^16.2.4",
		"@svgr/webpack": "^6.3.1",
		"@tailwindcss/forms": "^0.5.3",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@types/react": "^18.0.18",
		"autoprefixer": "^10.4.8",
		"commitizen": "^4.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"jest": "^27.5.1",
		"lefthook": "^2.0.1",
		"next-sitemap": "^2.5.28",
		"postcss": "^8.4.16",
		"tailwindcss": "^3.1.8",
		"typescript": "^4.8.2"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
